<main class="contenedor contenedor-formulario">
    <h1><%-nombrePagina%></h1>

    <form action="/iniciar-sesion" method="post" class="default-form">
        <div class="campo">
            <label for="">Email</label>
            <input type="email" name="email" placeholder="Tu email">
        </div>
        <div class="campo">
            <label for="">Password</label>
            <input type="password" name="password" placeholder="Tu password">
        </div>
        <div class="campo enviar">
            <input type="submit" value="Iniciar sesion" class="btn btn-rosa">
        </div>
    </form>
</main>